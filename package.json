        "lodash": "^4.17.13",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.15.2",
        "sass-loader": "^8.0.0",
        "svelte": "^3.1.0",
        "svelte-loader": "^2.13.4",
        "vue-template-compiler": "^2.6.11",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3"
    },
    "dependencies": {
        "@inertiajs/inertia": "^0.1.7",

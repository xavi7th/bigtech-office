{
	"compilerOptions": {
		"target": "es2017",
		"allowSyntheticDefaultImports": true,
		"baseUrl": "./",
		"paths": {
			"@public-pages/*": ["main/app/Modules/PublicPages/Resources/js/Pages/*"],
			"@public-shared/*": ["main/app/Modules/PublicPages/Resources/js/Shared/*"],
			"@public-assets/*": ["main/app/Modules/PublicPages/Resources/*"],
			"@user-pages/*": ["main/app/Modules/AppUser/Resources/js/Pages/*"],
			"@user-shared/*": ["main/app/Modules/AppUser/Resources/js/Shared/*"],
			"@user-assets/*": ["main/app/Modules/AppUser/Resources/*"],
			"@superadmin-pages/*": ["main/app/Modules/SuperAdmin/Resources/js/Pages/*"],
			"@superadmin-shared/*": ["main/app/Modules/SuperAdmin/Resources/js/Shared/*"],
			"@superadmin-assets/*": ["main/app/Modules/SuperAdmin/Resources/*"],
		}
	},
	"exclude": ["node_modules/*", "main/vendor/*", "main/storage/*"]
}
